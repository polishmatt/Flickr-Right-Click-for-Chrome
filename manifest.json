{
  "name": "Flickr Right Click",
  "version": "2.0.2",
  "description": "Lets you right click images on Flickr.",
  "icons": { 
    "16": "icon16.png",
    "32": "icon32.png",
    "48": "icon48.png"
  },

  "content_scripts": [
    {
      "matches": ["*://www.flickr.com/photos/*/*"],
      "css": ["base.css"]
    }
  ],

  "permissions": [
    "*://www.flickr.com/photos/*/*"
  ],

  "manifest_version": 2
}
